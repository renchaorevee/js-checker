{
    "name": "js-checker",
    "description": "JS precondition checking helpers",
    "version": "0.0.1",
    "homepage": "https://github.com/renchaorevee/js-checker",
    "keywords": [
        "precondition", "check", "JavaScript"
    ],
    "author": "Chao Ren <renchaorevee@gmail.com>",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "http://github.com/renchaorevee/js-checker.git"
    },
    "devDependencies": {
        "mocha": "latest",
        "istanbul": "latest",
        "jshint": "latest",
        "uglify-js": "latest",
        "coveralls": "latest",
        "contextify": "latest"
    },
    "scripts": {
        "test": "node ./node_modules/.bin/_mocha --reporter spec",
